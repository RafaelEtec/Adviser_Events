CREATE TABLE tb_ADVISER(
    ad_ID INT PRIMARY KEY AUTO_INCREMENT,
    ad_NOME VARCHAR(255) UNIQUE NOT NULL,
    ad_EMAIL VARCHAR(255) UNIQUE NOT NULL,
    ad_CPF VARCHAR(14) UNIQUE NOT NULL,
    ad_NUM VARCHAR(14),
    ad_NASC DATE,
    ad_ENDE VARCHAR(255),
    ad_PASS VARCHAR(36) NOT NULL
);

CREATE TABLE tb_USUARIO (
    us_ID INT PRIMARY KEY AUTO_INCREMENT,
    us_NOME VARCHAR(255) UNIQUE NOT NULL,
    us_NASC DATE,
    us_PASS VARCHAR(36) NOT NULL
);
